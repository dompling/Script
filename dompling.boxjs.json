    {
      "id": "Spotify",
      "name": "Spotify歌词翻译",
      "keys": [
        "BaiDu_APP_ID",
        "BaiDu_SECURITY_KEY",
        "@Spotify.music163",
        "@Spotify.cacheLyics",
        "@Spotify.autoTrans",
